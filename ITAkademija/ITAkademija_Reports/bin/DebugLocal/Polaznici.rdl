  <DataSets>
    <DataSet Name="Polaznici">
      <Query>
        <DataSourceName>ITAkademija</DataSourceName>
        <CommandText>SELECT Ime+' '+Prezime AS POLAZNIK,Spol,DATEDIFF(YEAR,DatumRodjenja,GETDATE()) AS Starost,CONVERT(DATE,Po.DatumUpisa)AS DatumUpisa,O.Naziv AS Oblast,K.Naziv AS Kurs
FROM Polaznici as P
INNER JOIN Pohadja AS Po ON Po.PolaznikID=P.PolaznikID
 INNER JOIN Kursevi AS K ON Po.KursID=K.KursID
 INNER JOIN Oblast AS O ON O.OblastID=K.OblastID
ORDER BY 6</CommandText>
      </Query>
      <Fields>
        <Field Name="POLAZNIK">
          <DataField>POLAZNIK</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
